<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" layout="2columns-left">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block cacheable="false" class="Riki\Subscription\Block\Frontend\Profile\View" template="subscription-profile-view.phtml">
            </block>
        </referenceContainer>
        <referenceContainer name="page.messages">
            <block class="Magento\Framework\View\Element\Messages" name="messages" as="messages"/>
        </referenceContainer>
    </body>
</page>