<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Riki_CsvOrderMultiple::import_order_csv" title="Import Multiple Orders" sortOrder="150">
                                <resource id="Riki_CsvOrderMultiple::import_order_csv_delete" title="Delete" sortOrder="150"/>
                                <resource id="Riki_CsvOrderMultiple::import_order_csv_import" title="Import" sortOrder="150"/>
                                <resource id="Riki_CsvOrderMultiple::import_order_csv_run_cron" title="Run Order Import Cron" sortOrder="150"/>
                                <resource id="Riki_CsvOrderMultiple::import_order_csv_download" title="Download CSV Files" sortOrder="150"/>
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>